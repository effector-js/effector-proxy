{
  "version": 2,
  "headers": [
    {
      "source": "/api/get-code",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "(/.*\\.js)",
      "destination": "/index.html"
    }
  ]
}
